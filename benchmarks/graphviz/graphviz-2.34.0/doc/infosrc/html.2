  TITLE="<I>value</I>"
  TOOLTIP="<I>value</I>"
  VALIGN="MIDDLE|BOTTOM|TOP"
  WIDTH="<I>value</I>"
&gt;
</PRE>
<P>
  &lt;!-- Table row --&gt;
<PRE>&lt;TR
  &lt;!-- No attributes --&gt;
&gt;
</PRE>
<P>
  &lt;!-- Table cell --&gt;
<PRE>&lt;TD
  ALIGN="CENTER|LEFT|RIGHT|TEXT"
  BALIGN="CENTER|LEFT|RIGHT"
  BGCOLOR="<I>color</I>"
  BORDER="<I>value</I>"
  CELLPADDING="<I>value</I>"
  CELLSPACING="<I>value</I>"
  COLOR="<I>color</I>"
  COLSPAN="<I>value</I>"
  FIXEDSIZE="FALSE|TRUE"
  GRADIENTANGLE="<I>value</I>"
  HEIGHT="<I>value</I>"
  HREF="<I>value</I>"
  ID="<I>value</I>"
  PORT="<I>portName</I>"
  ROWSPAN="<I>value</I>"
  STYLE="<I>value</I>"
  TARGET="<I>value</I>"
  TITLE="<I>value</I>"
  TOOLTIP="<I>value</I>"
  VALIGN="MIDDLE|BOTTOM|TOP"
  WIDTH="<I>value</I>"
&gt;
</PRE>

<P>
  &lt;!-- Font specification --&gt;
<PRE>&lt;FONT
